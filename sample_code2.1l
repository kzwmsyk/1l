# いくつかの予約語に短いバージョンがある。
# var: v
# fun: fn
# true: t
# false: f
# print: p
# class : cls
# method : mthd


# ブロックの最後の式文、print文、return文はセミコロンを省略可能
v a = 1;
fn b(i) { p a + i }
b(15);

fn ternary(bool) {return bool ? "TRUE" : "FALSE"}
p ternary(t);
p ternary(f);

# do while実装済み

var a = 3;
do {
    p a;
    a = a - 1;
} while (a < 0)

p a;

# プログラムの最後の式文、print文、return文もセミコロンを省略可能
p 1+1